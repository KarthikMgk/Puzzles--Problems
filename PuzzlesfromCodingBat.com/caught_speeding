def caught_speeding(speed, is_birthday):
  while is_birthday == True:
    if speed <= 60+5:
      return 0
    elif speed >=61+5 and speed<=80+5:
      return 1
    else:
      return 2
  else:
    if speed <= 60:
      return 0
    elif speed >=61 and speed<=80:
      return 1
    else:
      return 2
